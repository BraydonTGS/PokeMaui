<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="PokeMaui.Maui.MainPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PokeMaui.Maui.View"
    xmlns:model="clr-namespace:PokeMaui.Business.Models;assembly=PokeMaui.Business"
    xmlns:viewmodel="clr-namespace:PokeMaui.Maui.ViewModel"
    x:DataType="viewmodel:MainViewModel">

    <StackLayout>
        <local:PokemonView BindingContext="{Binding PokemonViewModel}" />
    </StackLayout>
</ContentPage>
